{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "ConnectionStrings": {
    "EduMatch": ""
  },


  "JwtSettings": {
    "SecretKey": "",
    "Issuer": "",
    "Audience": ""
  },

  "AllowedOrigins": [
    "",
    ""
  ],


  "CloudinarySettings": {

    "Env": "",
    "AppName": "",

    "Cloudinary": {
      "CloudName": "",
      "ApiKey": "",
      "ApiSecret": "",
      "UnsignedUploadPreset": "",
      "AssetFolder": ""
    },

    "Naming": {
      "UseHashedEmail": true,
      "OwnerKeyHexLength": 10,
      "ShardBuckets": 16,
      "UseUtcClock": true,
      "PublicIdPattern": ""
    },

    "MediaDelivery": {
      "Image": {
        "StoreMaxWidth": 1600,
        "DefaultWidth": 800,
        "DefaultHeight": 450,
        "DefaultCrop": "fill"
      },
      "Video": {
        "PosterWidth": 800,
        "PosterHeight": 450
      }
    },

    "UploadPolicy": {
      "Image": {
        "MaxSizeMB": 5,
        "MinWidth": 300,
        "MinHeight": 300,
        "MaxWidth": 6000,
        "MaxHeight": 6000,
        "AllowedContentTypes": [ "image/jpeg", "image/png", "image/webp" ],
        "AllowedExtensions": [ ".jpg", ".jpeg", ".png", ".webp" ]
      },
      "Video": {
        "MaxSizeMB": 50,
        "AllowedContentTypes": [ "video/mp4", "video/webm", "video/quicktime" ],
        "AllowedExtensions": [ ".mp4", ".webm", ".mov" ]
      }
    }
  }





}
